docker run --name productionDB -e MYSQL_ROOT_PASSWORD=pass -e MYSQL_DATABASE=productionDB -e MYSQL_USER=user -e MYSQL_PASSWORD=pass -p 3316:3306 -d mysql:latest
docker run --name testDB -e MYSQL_ROOT_PASSWORD=pass -e MYSQL_DATABASE=testDB -e MYSQL_USER=user -e MYSQL_PASSWORD=pass -p 3317:3306 -d mysql:latest

-Dspring.profiles.active=prod
-Dspring.profiles.active=test